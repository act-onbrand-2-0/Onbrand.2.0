# Supabase Configuration
SUPABASE_URL=https://pyvobennsmzyvtaceopn.supabase.co
SUPABASE_ANON_KEY=your_actual_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_actual_service_role_key_here

# Supabase Edge Functions
SUPABASE_FUNCTIONS_URL=https://pyvobennsmzyvtaceopn.functions.supabase.co

# AI Services (Required for AI Agent)
OPENAI_API_KEY=sk-your_openai_key_here
ANTHROPIC_API_KEY=sk-ant-your_anthropic_key_here
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_key_here

# AI SDK (these are auto-read by @ai-sdk providers)
# ANTHROPIC_API_KEY is used by @ai-sdk/anthropic
# OPENAI_API_KEY is used by @ai-sdk/openai
# GOOGLE_GENERATIVE_AI_API_KEY is used by @ai-sdk/google
REPLICATE_API_TOKEN=r8_your_replicate_token_here

# AI Model Selection (gemini | gpt4o | gpt4oMini | gpt5 | gpt5Mini | claude | claudeOpus | claudeHaiku)
# claude = Claude 3.5 Sonnet (Recommended - best quality)
# claudeOpus = Claude 3 Opus (Most capable)
# claudeHaiku = Claude 3.5 Haiku (Fast and cost-effective)
# Note: gpt5 and gpt5Mini are forward-compatible - they will work when OpenAI releases them
AI_MODEL=claude

# Social Analytics (getlate.dev)
SOCIAL_ANALYTICS_KEY=your_getlate_api_key_here
GETLATE_API_URL=https://api.getlate.dev

# Email Service (Resend)
RESEND_API_KEY=re_your_resend_api_key_here

# RAG & Document Processing
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
VECTOR_DIMENSIONS=1536

# n8n Workflow Automation
N8N_API_KEY=your_n8n_api_key_here
N8N_BASE_URL=https://your-instance.n8n.cloud
# Note: Each workflow has its own webhook URL stored in the database

# Next.js Public Variables
NEXT_PUBLIC_SUPABASE_URL=https://pyvobennsmzyvtaceopn.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_actual_anon_key_here

# Environment
NODE_ENV=development
NEXT_PUBLIC_APP_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
